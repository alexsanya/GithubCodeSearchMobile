<header>
  <div class="back-button left-button" {{action 'back'}}>
    Back
  </div>
  <h1>
    {{model.repo}}
  </h1>
</header>
<div id="main">
	<ul class="ui-table-view">
		{{#each model.matches.items as |item|}}
			<li {{action 'showFile' model.repo item model.line}} class="disclosure">{{item.path}}</li>
		{{/each}}
	</ul>
</div>